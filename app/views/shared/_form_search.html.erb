<!-- Barra de pesquisa-->
<%= form_with(url: _url,  method: :get, class: "form-group") do |form| %>
   <div class="form-group">
      <%= form.text_field :term, class:"form-control", placeholder:"Type #{type}..." %>
   </div>
   <div>
      <button type="submit" class="btn btn-primary">Search</button>
   </div>
<% end %>






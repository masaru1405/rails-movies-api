<h1>Keywords#index</h1>
<p>Find me in app/views/keywords/index.html.erb</p>


<% @keywords['results'].each do |keyword| %>
   <p class="keywords_text">
      <%= link_to keyword['name'], "#", class:"keywords_links" %>
   </p>
<% end %>

<div>
   <%= link_to "Previous", keywords_path(page: @page-1) %> |
   <%= link_to "Next", keywords_path(page: @page+1) %>
</div>